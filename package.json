{
  "name": "@fleeks-ai/sdk",
  "version": "0.5.0",
  "description": "TypeScript/JavaScript SDK for the Fleeks Development Platform",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fleeks",
    "sdk",
    "ai",
    "workspaces",
    "containers",
    "embeds",
    "cloud-ide"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fleeks-ai/fleeks-sdk-js.git"
  },
  "homepage": "https://fleeks.ai",
  "bugs": "https://github.com/fleeks-ai/fleeks-sdk-js/issues",
  "dependencies": {
    "socket.io-client": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
